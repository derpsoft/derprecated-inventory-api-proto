{
  "dependencies": {
    "Derprecated.Api.Models": "1.0.*",
    "Derprecated.Api": "1.0.*",
    "Derprecated.Api.Configuration.Core": "1.0.*",
    "NLog": "5.0.0-*",
    "NLog.Extensions.Logging": "1.0.0-*",
    "System.Net.Security": "4.3.*",
    "Microsoft.AspNetCore.Diagnostics": "1.1.*",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.1.*",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.*",
    "Microsoft.Extensions.Configuration": "1.1.*",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.1.*",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.1.*",
    "Microsoft.Extensions.Configuration.Json": "1.1.*",
    "Microsoft.Extensions.Logging.Console": "1.1.*",
    "Microsoft.NETCore.App": {
      "version": "1.1.*",
      "type": "platform"
    },
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.1.*",
    "ServiceStack.Text.Core": "1.0.37",
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview2-final"
  },
  "runtimes": {
    "win7-x64": {}
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },
  "publishOptions": {
    "include": [
      "wwwroot",
      "web.config",
      "appsettings.json",
      "nlog.config"
    ]
  },
  "scripts": {
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  },
  "configurations": {
    "Production": {
    }
  }
}
